<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import org.controlsfx.control.StatusBar?>

<GridPane styleClass="grid-pane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <StatusBar fx:id="syncStatus" styleClass="anchor-pane"/>
    <StatusBar styleClass="anchor-pane" fx:id="totalPersons" GridPane.columnIndex="1"/>
    <StatusBar styleClass="anchor-pane" fx:id="saveLocationStatus" GridPane.columnIndex="2"
               nodeOrientation="RIGHT_TO_LEFT"/>
    <rowConstraints>
        <RowConstraints/>
    </rowConstraints>
</GridPane>
