<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane styleClass="grid-pane" stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="600.0" prefWidth="800.0">
         <children>
            <VBox alignment="TOP_CENTER" prefWidth="150.0" spacing="50.0">
               <children>
                  <Label />
                  <Label styleClass="label-header" text="Mon" />
                  <Label styleClass="label-header" text="Tut" />
                  <Label styleClass="label-header" text="Wed" />
                  <Label styleClass="label-header" text="Thu" />
                  <Label styleClass="label-header" text="Fri" />
               </children>
               <HBox.margin>
                  <Insets top="-5.0" />
               </HBox.margin>
            </VBox>
            <VBox prefWidth="650.0">
               <children>
                  <HBox fx:id="timeHeader" alignment="CENTER" prefHeight="80.0" spacing="12.0">
                     <children>
                        <Label styleClass="cell_small_label" text="0800" />
                        <Label styleClass="cell_small_label" text="0900" />
                        <Label styleClass="cell_small_label" text="1000" />
                        <Label styleClass="cell_small_label" text="1100" />
                        <Label styleClass="cell_small_label" text="1200" />
                        <Label styleClass="cell_small_label" text="1300" />
                        <Label styleClass="cell_small_label" text="1400" />
                        <Label styleClass="cell_small_label" text="1500" />
                        <Label styleClass="cell_small_label" text="1600" />
                        <Label styleClass="cell_small_label" text="1700" />
                        <Label styleClass="cell_small_label" text="1800" />
                        <Label styleClass="cell_small_label" text="1900" />
                        <Label styleClass="cell_small_label" text="2000" />
                     </children>
                  </HBox>
                  <GridPane fx:id="timetableGrid" prefHeight="520.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="110.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="110.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="110.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="110.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="110.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                  </GridPane>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</StackPane>
