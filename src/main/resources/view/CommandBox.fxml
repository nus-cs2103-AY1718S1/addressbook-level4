<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="stackPane" styleClass="anchor-pane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="keywordLabel" text="Label" />
   <Text fx:id="commandText" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="JUSTIFY" StackPane.alignment="CENTER_LEFT">
      <StackPane.margin>
         <Insets left="13.0" />
      </StackPane.margin>
   </Text>
   <TextField fx:id="commandTextFieldKeyword" alignment="CENTER" editable="false" maxWidth="-Infinity" StackPane.alignment="CENTER_LEFT">
      <StackPane.margin>
         <Insets left="13.0" />
      </StackPane.margin></TextField>
  <TextField fx:id="commandTextField" onAction="#handleCommandInputChanged" onKeyPressed="#handleKeyPress" onKeyReleased="#handleKeyReleased" promptText="Enter command here..." StackPane.alignment="CENTER_LEFT" />
</StackPane>
